  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build": "bash scripts/build-production.sh",
    "build:verify": "tsx scripts/verify-build.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx scripts/seed.ts",
    "db:migrate": "tsx scripts/run-migrations.ts up",
    "db:migrate:status": "tsx scripts/run-migrations.ts status",
    "db:migrate:verify": "tsx scripts/run-migrations.ts verify",
    "db:migrate:rollback": "tsx scripts/run-migrations.ts rollback",
    "db:migrate:create": "tsx scripts/create-migration.ts",
    "backup": "tsx scripts/backup.ts backup",
    "backup:restore": "tsx scripts/backup.ts restore",
    "validate:env": "tsx scripts/validate-env.ts",
    "validate:db": "tsx scripts/validate-database.ts",
    "test": "NODE_ENV=test vitest run --config vitest.config.server.ts",
    "test:watch": "NODE_ENV=test vitest --config vitest.config.server.ts",
    "test:server": "NODE_ENV=test vitest run --config vitest.config.server.ts",
    "test:client": "NODE_ENV=test vitest run --config vite.config.ts",
    "test:client:watch": "NODE_ENV=test vitest --config vite.config.ts",
    "test:client:ui": "NODE_ENV=test vitest --ui --config vite.config.ts",
    "test:coverage": "NODE_ENV=test vitest run --coverage --config vitest.config.server.ts && vitest run --coverage --config vite.config.ts",
    "test:ui": "NODE_ENV=test vitest --ui --config vitest.config.server.ts",
    "deploy:check": "npm run validate:env && npm run check && npm run validate:db"
  },
